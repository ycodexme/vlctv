<!--
  ~ *************************************************************************
  ~  French TV Loading Icon
  ~ **************************************************************************
  ~ Modified for French TV IPTV App
  ~ **************************************************************************
  -->

<vector android:height="46dp"
        android:viewportHeight="130"
        android:viewportWidth="130"
        android:width="46dp"
        xmlns:android="http://schemas.android.com/apk/res/android">
    
    <!-- TV Body -->
    <path
            android:fillColor="#3A4A5C"
            android:pathData="M35,40 L95,40 L95,85 L35,85 Z" />
    
    <!-- TV Antenna Left -->
    <path
            android:strokeColor="#3A4A5C"
            android:strokeWidth="4"
            android:strokeLineCap="round"
            android:pathData="M50,40 L40,25" />
    
    <!-- TV Antenna Right -->
    <path
            android:strokeColor="#3A4A5C"
            android:strokeWidth="4"
            android:strokeLineCap="round"
            android:pathData="M80,40 L90,25" />
    
    <!-- TV Stand Left -->
    <path
            android:strokeColor="#3A4A5C"
            android:strokeWidth="4"
            android:strokeLineCap="round"
            android:pathData="M50,85 L45,100" />
    
    <!-- TV Stand Right -->
    <path
            android:strokeColor="#3A4A5C"
            android:strokeWidth="4"
            android:strokeLineCap="round"
            android:pathData="M80,85 L85,100" />
    
    <!-- French Flag - Blue -->
    <path
            android:fillColor="#0055A4"
            android:pathData="M40,45 L55,45 L55,80 L40,80 Z" />
    
    <!-- French Flag - White -->
    <path
            android:fillColor="#FFFFFF"
            android:pathData="M55,45 L70,45 L70,80 L55,80 Z" />
    
    <!-- French Flag - Red -->
    <path
            android:fillColor="#EF4135"
            android:pathData="M70,45 L90,45 L90,80 L70,80 Z" />
</vector>
